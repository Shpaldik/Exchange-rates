{
    "version": 2,
    "builds": [
      {
        "src": "proxy-server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/cryptocurrency/listings/latest",
        "dest": "/proxy-server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  