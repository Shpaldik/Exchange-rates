{
    "version": 2,
    "builds": [
      {
        "src": "proxy-server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/v1/cryptocurrency/listings/latest",
        "dest": "/proxy-server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  